<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:Label text="UDP Implementation Guide" style="-unity-text-align: upper-center; color: rgb(109, 217, 238); font-size: 15px; border-top-width: 10px;" />
    <ui:Label text="&#10;&#10;Answer these quick questions so we can better guide you in your UDP implementation.&#10;&#10;&#10;" display-tooltip-when-elided="true" style="white-space: normal; border-left-width: 5px;" />
    <ui:VisualElement style="display: flex; border-left-width: 10px;">
