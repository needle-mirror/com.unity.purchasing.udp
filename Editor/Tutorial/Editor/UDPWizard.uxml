        <ui:Toggle label="No" name="IAPNo" value="false" style="white-space: normal;" />
    </ui:VisualElement>
    <ui:Label text="&#10;&#10;" display-tooltip-when-elided="true" style="border-left-width: 10px; display: flex; visibility: hidden;" />
